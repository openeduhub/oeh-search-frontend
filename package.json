{
  "name": "open-edu-hub-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "graphql-codegen -w & ng serve",
    "clean": "rm -rf dist",
    "prebuild": "graphql-codegen",
    "prebuild:dev": "graphql-codegen",
    "prebuild:stage": "graphql-codegen",
    "build": "ng build --prod --localize",
    "build:dev": "ng build --localize",
    "build:stage": "ng build --configuration=stage --localize",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:local:open": "cd e2e-cypress & npm run cypress:local:open",
    "format": "prettier --write src",
    "docker-build": "docker build . --tag open-edu-hub-frontend",
    "docker-run": "docker run --name open-edu-hub-frontend --rm -ti -p 8080:80 open-edu-hub-frontend",
    "extract-i18n": "ng xi18n open-edu-hub-frontend --output-path src/locale && ng run open-edu-hub-frontend:xliffmerge",
    "start-de": "ng serve --configuration=de",
    "generate": "graphql-codegen",
    "postinstall": "patch-package && ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.3",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/localize": "^10.0.3",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "~10.0.3",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@openfonts/montserrat_all": "^1.44.1",
    "angular-oauth2-oidc": "^9.2.1",
    "apollo-angular": "^1.9.1",
    "apollo-angular-link-http": "^1.10.0",
    "apollo-angular-link-http-batch": "^1.8.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "material-icons": "^0.3.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.2",
    "@angular/cli": "~10.0.2",
    "@angular/compiler-cli": "~10.0.3",
    "@angular/language-service": "~10.0.3",
    "@graphql-codegen/cli": "1.13.3",
    "@graphql-codegen/fragment-matcher": "^1.16.2",
    "@graphql-codegen/typescript": "1.13.3",
    "@graphql-codegen/typescript-apollo-angular": "1.13.3",
    "@graphql-codegen/typescript-operations": "1.13.3",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.1.1",
    "patch-package": "^6.2.2",
    "prettier": "^2.0.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,md,html,css,scss}": "prettier --write",
    "*.ts": "tslint --fix"
  }
}
