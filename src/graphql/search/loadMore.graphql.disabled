query LoadMoreSources(
  $size: Int!
  $searchString: String!
  $filters: [Filter!]
  $language: Language!
) {
  facets(searchString: $searchString, size: $size, filters: $filters, language: $language) {
    sources {
      ...aggregation
    }
  }
}

